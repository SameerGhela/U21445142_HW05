@{
    ViewBag.Title = "About";
}
@using Homework5.Models;
@model List<Students>
<h3><b>Students</b></h3>
<hr />
@if (Model.Count() == 0)
{
    <p>No data</p>
}
else
{
 
<table class="table">
    <tr>
    <th><b>#</b></th>
    <th><b>Name</b></th>
    <th><b>Surname</b></th>
    <th><b>Class</b></th>
    <th><b>Points</b></th>
    <th><b>Borrow Book</b></th>
        </tr>
    @foreach(var student in Model)
                    {
                        <tr>
                            <td>@student.StudentID</td>
                            <td> @student.StudentName</td>
                            <td> @student.StudentSurname</td>
                            <td> @student.Class</td>
                            <td> @student.Point</td>
                        </tr>
                    }
</table>
    }